@{ Layout = "~/Views/Shared/_Layout.cshtml"; }
<style type="text/css">
    body {
        background-image: url(/Content/images/right_bg.png);
        background-repeat: no-repeat;
        background-size: 100% 100%;
    }
</style>
<!-- 顶部开始 -->
<div class="container" style="background-image:url(/Content/images/top_bg.png);background-repeat:no-repeat; background-size:100% 100%;">
    <div class="logo"><a href="/Menu/Home"><font style="color:#175143;font-size:18px;font-weight:bold;font-family:'Microsoft YaHei';"></font></a></div>
    <div class="left_open">
