@{
    ViewBag.Title = "用户管理";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="x-nav">
    <span class="layui-breadcrumb">
        <a href="">首页</a>
        <a href="">演示</a>
        <a>
            <cite>导航元素</cite>
        </a>
    </span>
    <a class="layui-btn layui-btn-small" style="line-height:1.6em;margin-top:3px;float:right" onclick="location.reload()" title="刷新">
        <i class="layui-icon layui-icon-refresh" style="line-height:30px"></i>
    </a>
</div>
<div class="layui-fluid">
    <div class="layui-row layui-col-space15">
        <div class="layui-col-md12">
            <div class="layui-card">
                <div class="layui-card-header">
                    <button class="layui-btn" lay-submit="" lay-filter="sreach"><i class="layui-icon"></i>增加</button>
                    <button class="layui-btn layui-btn-danger" onclick="delAll()">
                        <i class="layui-icon"></i>批量删除
                    </button>

                </div>
                <div class="layui-card-body ">
                    <table class="layui-table layui-form" id="list">
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="x-body">
    <div class="layui-row">
        <div class="layui-form layui-col-md12 x-so">
            <button class="layui-btn" type="button" onclick="x_admin_show('添加栏目','@Url.Action("Edit","Column")',600,520)" style="float:left;background-color: #1b8fe4;">添加栏目</button>

            <button class="layui-btn" type="button" onclick="" style="float:left;margin-left:10px;background-color: #1b8fe4;">批量删除</button>
        </div>
    </div>
    <div id="content">
        <table class="layui-table" id="list" lay-filter="test">
        </table>

    </div>

</div>
<script type="text/html" id="barDemo">
    <a class="layui-btn layui-btn-xs" lay-event="edit">编辑</a>
    <a class="layui-btn layui-btn-danger layui-btn-xs" lay-event="del">删除</a>
</script>

<script src="~/Scripts/Column/column.js?@DateTime.Now.ToString("yyyymmddhhMMss")"></script>
